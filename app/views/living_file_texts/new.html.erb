<h1><%= @living_file.title %> :: Textos</h1>

<% form_for([@living_file, @living_file_text], :html => {:multipart => true }) do |f| %>
  <%= f.error_messages %>
	
	<%= render :partial => "form", :locals => { :f => f } %>
  
  <p>
    <%= f.submit 'Criar', :disable_with => "Criando.." %>
  </p>
<% end %>

<%= link_to 'Voltar', living_file_living_file_texts_path(@living_file) %>