<h1>Listing medialibrary_abouts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

<% @medialibrary_abouts.each do |medialibrary_about| %>
  <tr>
    <td><%=h medialibrary_about.title %></td>
    <td><%=h medialibrary_about.description %></td>
    <td><%= link_to 'Show', medialibrary_about %></td>
    <td><%= link_to 'Edit', edit_medialibrary_about_path(medialibrary_about) %></td>
    <td><%= link_to 'Destroy', medialibrary_about, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New medialibrary_about', new_medialibrary_about_path %>