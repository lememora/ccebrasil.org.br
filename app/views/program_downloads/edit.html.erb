<h1>Programação Download :: Edição</h1>

<% form_for(@program_download, :html => {:multipart => true }) do |f| %>
  <%= f.error_messages %>

	<%= render :partial => "form", :locals => { :f => f } %>
	
  <p>
    <%= f.submit 'Atualizar', :disable_with => "Atualizando.." %>
  </p>
<% end %>

<%= link_to 'Voltar', program_downloads_path %>